  "classes": [
    {
      "uri": "http://www.w3.org/ns/adms#Asset",
      "name": "Asset",
      "label": {
        "en": "Asset"
      },
      "sort": "Asset",
      "description": {
        "en": "An abstract entity that reflects the intellectual content of the asset and represents those characteristics of the asset that are independent of its physical embodiment. This abstract entity combines the FRBR entities work (a distinct intellectual or artistic creation) and expression (the intellectual or artistic realization of a work)"
      },
      "usage": {},
      "equivalent": [],
      "parents": []
    },
    {
      "uri": "http://www.w3.org/ns/adms#AssetDistribution",
      "name": "AssetDistribution",
      "label": {
        "en": "Asset Distribution"
      },
      "sort": "Asset Distribution",
      "description": {
        "en": "A particular physical embodiment of an Asset, which is an example of the FRBR entity manifestation (the physical embodiment of an expression of a work)."
      },
      "usage": {},
      "equivalent": [],
      "parents": []
    },
    {
      "uri": "http://www.w3.org/ns/adms#AssetRepository",
      "name": "AssetRepository",
      "label": {
        "en": "Asset repository"
      },
      "sort": "Asset repository",
      "description": {
        "en": "A system or service that provides facilities for storage and maintenance of descriptions of Assets and Asset Distributions, and functionality that allows users to search and access these descriptions. An Asset Repository will typically contain descriptions of several Assets and related Asset Distributions."
      },
      "usage": {},
      "equivalent": [],
      "parents": []
    },
