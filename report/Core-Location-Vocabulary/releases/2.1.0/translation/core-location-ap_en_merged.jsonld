    {
      "@id": "http://www.w3.org/ns/locn#Address",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "EA-Guid": "{650BCC62-B5E6-4070-8DC6-A39F0953E191}",
      "ap-definition-en": {
        "en": "A spatial object that in a human-readable way identifies a fixed location."
