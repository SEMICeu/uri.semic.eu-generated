      <xs:element name="description" type="xs:string" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">A free-text account of the record. This property can be repeated for parallel language versions of the description.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="listingDate" type="xs:string" maxOccurs="1" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">The date on which the description of the Resource was included in the Catalogue.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="language" ref="LinguisticSystemType" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">A language used in the textual metadata describing titles, descriptions, etc. of the Catalogued Resource.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="modificationDate" type="xs:string" maxOccurs="1" minOccurs="1">
