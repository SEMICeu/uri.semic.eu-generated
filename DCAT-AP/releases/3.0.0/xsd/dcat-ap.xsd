        </xs:annotation>
      </xs:element>
      <xs:element name="hasPart" ref="CatalogueType" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">A related Catalogue that is part of the described Catalogue.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="releaseDate" type="xs:string" maxOccurs="1" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">The date of formal issuance (e.g., publication) of the Catalogue.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="language" ref="LinguisticSystemType" maxOccurs="unbounded" minOccurs="0">
