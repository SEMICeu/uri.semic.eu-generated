          <xs:documentation xml:lang="en" source="#usage">May be repeated for different versions of the name</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="ConceptschemeRef" type="xs:IDREF">
      <xs:annotation>
        <xs:documentation xml:lang="en">Reference to Conceptscheme ID</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
