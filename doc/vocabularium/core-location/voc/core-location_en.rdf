  <owl:DatatypeProperty rdf:about="http://data.europa.eu/m8g/crs">
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/m8g"/>
    <rdfs:label xml:lang="en">crs</rdfs:label>
    <rdfs:comment xml:lang="en">An identifier for the coordinate reference system.</rdfs:comment>
    <skos:scopeNote xml:lang="en">Can be a coordinate-based local, regional or global system used to locate geographical entities.</skos:scopeNote>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/locn#Geometry"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="http://data.europa.eu/m8g/geometryType">
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/m8g"/>
    <rdfs:label xml:lang="en">geometry type</rdfs:label>
    <rdfs:comment xml:lang="en">The classification of the Geometry.</rdfs:comment>
    <skos:scopeNote xml:lang="en">Can be a point, line or polygon.</skos:scopeNote>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/locn#Geometry"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
