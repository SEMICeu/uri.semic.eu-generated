  <owl:DatatypeProperty rdf:about="http://data.europa.eu/m8g/coordinates">
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/m8g"/>
    <rdfs:label xml:lang="en">coordinates</rdfs:label>
