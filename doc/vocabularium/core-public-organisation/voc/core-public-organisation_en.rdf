  <owl:ObjectProperty rdf:about="http://data.europa.eu/m8g/frequency">
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/m8g"/>
    <rdfs:label xml:lang="en">frequency</rdfs:label>
    <rdfs:comment xml:lang="en">The recurrence of an instant or period.</rdfs:comment>
    <skos:scopeNote xml:lang="en">It is recommended to use the frequency from an existing vocabulary like the &lt;a href="https://op.europa.eu/en/web/eu-vocabularies/concept-scheme/-/resource?uri=http://publications.europa.eu/resource/authority/frequency"&gt;Frequency&lt;/a&gt; from the Publications Office.</skos:scopeNote>
    <rdfs:domain rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </owl:ObjectProperty>
