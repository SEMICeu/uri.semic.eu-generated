    <rdfs:domain rdf:resource="http://example.com/rrmv/Request"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.com/rrmv/isIncomplete">
    <rdfs:isDefinedBy rdf:resource="http://example.com/rrmv"/>
