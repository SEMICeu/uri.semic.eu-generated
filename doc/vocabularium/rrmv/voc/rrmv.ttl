@prefix dc: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.com/rrmv/ActionResult> a owl:Class;
  rdfs:label "Action Result"@en;
  rdfs:comment "The Result of an Action. It could be an activity (e.g., to do something) or an output (e.g., report)."@en;
  rdfs:isDefinedBy <http://example.com/rrmv> .

<http://example.com/rrmv/AgentRole> a owl:Class;
  rdfs:label "Agent Role"@en;
  rdfs:comment "The property linking a Agent to Role."@en;
  rdfs:isDefinedBy <http://example.com/rrmv> .

<http://example.com/rrmv/AgentType> a owl:Class;
  rdfs:label "Agent Type"@en;
  rdfs:comment "The type of Agent."@en;
  rdfs:isDefinedBy <http://example.com/rrmv>;
  rdfs:subClassOf skos:Concept .

<http://example.com/rrmv/Frequency> a owl:Class;
  rdfs:label "Frequency"@en;
  rdfs:comment "Frequency of an Request over time (e.g., every 6 months, each quarter)."@en;
  rdfs:isDefinedBy <http://example.com/rrmv> .

<http://example.com/rrmv/ResultType> a owl:Class;
  rdfs:label "Result Type"@en;
  rdfs:comment "Result Type. SKOS Concept."@en;
  rdfs:isDefinedBy <http://example.com/rrmv>;
  rdfs:subClassOf skos:Concept .

<http://example.com/rrmv/Role> a owl:Class;
  rdfs:label "Role"@en;
  rdfs:comment "Role is played by and Agent involved in the Request."@en;
  rdfs:isDefinedBy <http://example.com/rrmv> .

<http://example.com/rrmv/RoleType> a owl:Class;
  rdfs:label "Role Type"@en;
  rdfs:comment "The kind of role of the agent as is described in the legal resource."@en;
  rdfs:isDefinedBy <http://example.com/rrmv>;
  rdfs:subClassOf skos:Concept .

<http://example.com/rrmv/Status> a owl:Class;
  rdfs:label "Status"@en;
  rdfs:comment "Status of Action."@en;
  rdfs:isDefinedBy <http://example.com/rrmv>;
  rdfs:subClassOf skos:Concept .

<http://example.com/rrmv/TemporalFunction> a owl:Class;
  rdfs:label "Temporal Function"@en;
  rdfs:comment "Function of the temporal parameter."@en;
  rdfs:isDefinedBy <http://example.com/rrmv>;
  rdfs:subClassOf skos:Concept .

<http://example.com/rrmv/hasAnnotation> a owl:DatatypeProperty;
  rdfs:label "has annotation"@en;
  rdfs:comment "Annotation related to the Action or Request made by the domain expert."@en;
  rdfs:domain <http://example.com/rrmv/Request>,
    <http://example.com/rrmv/Action>;
  rdfs:isDefinedBy <http://example.com/rrmv>;
  rdfs:range xsd:string .

<http://example.com/rrmv/hasTopic> a owl:ObjectProperty;
  rdfs:label "has topic"@en;
  rdfs:comment "The topic of the request."@en;
  rdfs:domain <http://example.com/rrmv/Request>;
  rdfs:isDefinedBy <http://example.com/rrmv>;
  rdfs:range skos:Concept .

<http://example.com/rrmv/hasURI> a owl:DatatypeProperty;
  rdfs:label "has URI"@en;
  rdfs:comment "URI of the Request."@en;
  rdfs:domain <http://example.com/rrmv/Request>;
  rdfs:isDefinedBy <http://example.com/rrmv>;
  rdfs:range xsd:anyURI .

<http://example.com/rrmv/isIncomplete> a owl:DatatypeProperty;
  rdfs:label "is incomplete"@en;
  rdfs:comment "Data property that track the incompleteness of the temporal parameters."@en;
  rdfs:domain <http://www.w3.org/2006/time#ProperInterval>;
  rdfs:isDefinedBy <http://example.com/rrmv>;
  rdfs:range xsd:boolean .

<http://example.com/rrmv/Action> a owl:Class;
  rdfs:label "Action"@en;
  rdfs:comment "An event with at least one agent that is participant in it in a given time."@en;
  rdfs:isDefinedBy <http://example.com/rrmv> .

<http://example.com/rrmv/Request> a owl:Class;
  rdfs:label "Request"@en;
  rdfs:comment "A portion of legal source expression including a mandate that produces at least one Action."@en;
  rdfs:isDefinedBy <http://example.com/rrmv> .

<http://example.com/rrmv> a owl:Ontology;
  rdfs:label "rrmv"@en,
    "rrmv"@nl;
  dc:issued "2024-05-31";
  dc:license <https://creativecommons.org/licenses/by/4.0/>;
  dc:mediator [
    foaf:homepage <https://semic.eu>;
    foaf:name "Semantic Interoperability Community (SEMIC)"
  ] .
