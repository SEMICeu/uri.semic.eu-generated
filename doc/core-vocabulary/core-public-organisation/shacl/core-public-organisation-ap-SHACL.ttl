<https://semiceu.github.io//shacl/core-public-organisation-ap-SHACL_en#TemporalEntityShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:datatype rdf:langString;
    shacl:description "A  textual representation of the Temporal Entity."@en;
    shacl:name "description"@en;
    shacl:path dc:description
  ], [
    shacl:class skos:Concept;
