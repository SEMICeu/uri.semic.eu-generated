    shacl:path dc:type
  ];
  shacl:targetClass <http://data.europa.eu/m8g/Role> .

<https://semiceu.github.io//shacl/core-public-event-ap-SHACL_en#TemporalEntityShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:datatype rdf:langString;
    shacl:description "A  textual representation of the Temporal Entity"@en;
    shacl:name "description"@en;
    shacl:path dc:description
  ], [
    shacl:class <http://fixme.com#Frequency>;
    shacl:description "The recurrence of the temporal entity"@en;
    shacl:name "frequency"@en;
